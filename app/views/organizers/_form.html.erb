<%= semantic_form_for @organizer, :html => {:multipart => true} do |f| %>
  <%= f.error_messages %>
  <%= f.inputs do %>  
    <%= f.input :name %>
    <%= f.input :intro %>
    <%= f.input :description %>
    <%= f.input :website %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :logotype %>
    <%= f.input :photo %>
    <%= f.input :users, :as => :hidden %>
  <% end %>
 
 
  <p><%= f.buttons %></p>
    
<% end %>
