<% title t('organizers.index.title') %>

<table>
  <% for organizer in @organizers %>
    <tr>
      <td><%= link_to organizer.name, organizer %></td>
      <td><%= organizer.website %></td>
    </tr>
  <% end %>
</table>

<% if current_user %>
  <p><%= link_to t('.new'), new_organizer_path %></p>
<% end %>
