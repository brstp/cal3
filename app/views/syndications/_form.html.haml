= semantic_form_for @syndication do |f|
  = f.inputs do
    = f.input :organizer, :required => true, :collection => Organizer.all, :include_blank => false
    = f.input :syndicated_organizer, :required => true, :collection => Organizer.all, :include_blank => false

  = f.actions do
    = f.action :submit, :as => :input